<script id="table-assigned" type="text/x-handlebars-template">
    <div class="well-table">
        <h4>{% trans %} Assigned {% endtrans %}</h4>
        <table class="table table-hover table-striped">
            <thead>
                <th> {% trans %} Date {% endtrans %}</th>
                {% verbatim %}
                    {{#each headers}}
                        <th>{{ this }}</th>
                    {{/each}}
                {% endverbatim %}
            </thead>
            <tbody>
                {% verbatim %}
                {{#each assigned.dates}}
                <tr>
                    <td>{{@key}}</td>
                    {{#each this}}
                        <td>{{this}}</td>
                    {{/each}}
                </tr>
                {{/each}}
                {% endverbatim %}
            </tbody>
        </table>
    </div>
</script>
